@model projectWEB.ViewModels.ProductsViewModel

@if (Model != null)
{
    Html.RenderPartial("SearchFilters/_SearchInputFilter", Model);
    Html.RenderPartial("SearchFilters/_CategoriesFilter", Model);
    @await Component.InvokeAsync("PriceRangeFilter", new { priceFrom = Model.PriceFrom, priceTo = Model.PriceTo });
}
