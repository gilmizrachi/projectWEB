<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>History</title>
</head>
<body>
<table class="table">
    <thead>
        <tr>
            <th>
                Date
            </th>
        </tr>
    </thead>
    <tbody>
      @foreach (var item in ViewBag.History) {
             <tr>
                  <td>
                      <a href="@Url.Action("Details","Orders", new { date = item.Date })">@item.Date.ToString().Replace(" 0:00:00","")</a>
                  </td>
             </tr>
      }
    </tbody>
</table>
</body>
</html>
